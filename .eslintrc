{
    "extends": "eslint:recommended",
    "rules": {              // 1 means rule is being followed
            "no-alert": 1,
            "camelcase": 1,
            "curly": 1,
            "eqeqeq": 1,
            "no-console": 1,
            "guard-for-in": 1,
            "no-empty": 1,
            "no-param-reassign": 1,
            "no-unused-vars": 1,
            "quotes": [ 1, "single"]
    },
    "globals": {
        "jQuery": false,
        "$": false
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }
